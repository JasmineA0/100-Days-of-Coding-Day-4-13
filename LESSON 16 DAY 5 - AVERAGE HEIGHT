student_heights = input().split()
for n in range(0, len(student_heights)):
    student_heights[n] = int(student_heights[n])

total_height = 0
num_heights = 0

for height in student_heights:
    total_height += height
    num_heights += 1

average_height = total_height / num_heights
rounded_average_height = round(average_height)

print(f"Average height rounded to the nearest whole number: {rounded_average_height}")
